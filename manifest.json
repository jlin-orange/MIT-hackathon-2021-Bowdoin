{
    "name": "Take 5",
    "description": "This extension eliminates distractions on your computer for a 5 minute break!",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {"128": "icon128.png"},
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "browser_action": {
        "default_icon": "icon19.png"
    },
    "permissions": ["tabs"]
}